!- source: https://draculatheme.com/xresources
!- source: https://github.com/majutsushi/urxvt-font-size/
!- Author: Supamafia
!- data: Mar. 22 2023
!- Version: 0.1

!--------------------xdefauls
URxvt.font: xft:UbuntuMono Nerd Font Mono:size=11
URxvt.termName: rxvt
URxvt.scrollBar: false
URxvt.cursorBlink: true

! Dracula Xresources palette
*.foreground: #F8F8F2
*.background: #282A36
*.color0:     #000000
*.color8:     #4D4D4D
*.color1:     #FF5555
*.color9:     #FF6E67
*.color2:     #50FA7B
*.color10:    #5AF78E
*.color3:     #F1FA8C
*.color11:    #F4F99D
*.color4:     #BD93F9
*.color12:    #CAA9FA
*.color5:     #FF79C6
*.color13:    #FF92D0
*.color6:     #8BE9FD
*.color14:    #9AEDFE
*.color7:     #BFBFBF
*.color15:    #E6E6E6

URxvt.perl-ext-common: default,selection-to-clipboard,font-size
URxvt.keysym.C-+: font-size:increase
URxvt.keysym.C--: font-size:decrease
